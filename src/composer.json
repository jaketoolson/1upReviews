{
  "name": "orion/oneupreviews",
  "type": "project",
  "require": {
    "php": "^7.1.3",
    "cloudcreativity/laravel-json-api": "^0.12.0",
    "fideloper/proxy": "^4.0",
    "justinrainbow/json-schema": "^5.2",
    "laravel/framework": "5.6.14"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.4",
    "cloudcreativity/json-api-testing": "^0.3.0",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "laravel/homestead": "^7.0",
    "laravel/tinker": "^1.0",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "OneUpReviews\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OneUpReviews\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-update-cmd": [
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
